package arrayIndexOutOfBoundsException;

import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		String[] vetor = new String[5];

		Scanner sc = new Scanner(System.in);
		
		for (int i = 0; i < 5; i++) {
			String valor = sc.nextLine();
			vetor[i] = valor;
		}
		
	}

}

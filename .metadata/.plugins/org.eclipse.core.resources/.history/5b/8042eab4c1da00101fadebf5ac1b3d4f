package exercicio4FileReader;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		File file = new File("C:\\exerciciosFile\\Transacoes.txt");
		
		try(Scanner sc =new Scanner(file)){
			sc.useDelimiter(";");
			while(sc.hasNext()) {
				String palavra = sc.next();
				String.valueOf(palavra);
				try {
					int n=Integer.parseInt(palavra);
					System.out.println("Quantidade: " + n);
				}catch(NumberFormatException e) {
				}
				try {
					double m=Double.parseDouble(palavra);
					System.out.println("Pre√ßo: " + m);
				}catch(NumberFormatException e) {
					
				}
			}
			
			
		}catch(FileNotFoundException e) {
			System.out.println(e.getMessage());
		}

	}

}

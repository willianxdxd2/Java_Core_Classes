package entities;

public class MinhaThread2 extends Thread{
	@Override
	public void run() {
	for(int i =1 ;i <=100;i= i+ 12) {
		System.out.println("Baixando...");
		try {Thread.sleep(5000);}catch(InterruptedException e) {
			System.out.println("Erro no download1" + e.getMessage());
			break;}
	}
	}}
